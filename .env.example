# InfluScope Environment Variables
# Copy this file to .env and update values

# Database
DATABASE_URL=postgres://user:password@localhost:5432/influencer_db?sslmode=disable
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=influencer_db

# Elasticsearch
ELASTIC_URL=http://localhost:9200

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@localhost:5672/
RABBITMQ_DEFAULT_USER=guest
RABBITMQ_DEFAULT_PASS=guest

# Server
API_PORT=8080
SCRAPER_PORT=8081
INDEXER_PORT=8082

# Logging
LOG_LEVEL=info

